{
  "knobs": {
    "maxDims":  2,
    "sizeRange": [100, 1000],
    "loopDepthRange": [2, 4],
    "nestedLoopRange": [1, 3],
    "noLoopRange": [10, 15],
    "expressionDepthRange": [2, 4],
    "noOfFunctions": 1,
    "allDTypes": ["int", "uint", "long", "ulong", "char"],
    "targetDTypes": ["int", "uint"],
    "useModsInOuterLoop": true,
    "repeatFactor": 20,
    "allowReduction": false
  }
}
