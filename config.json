{
  "knobs": {
    "maxDims":  2,
    "noOfSingletonRange": [2, 5],
    "noOfArrayRange": [2, 5],
    "sizeRange": [100, 1000],
    "loopDepthRange": [3, 3],
    "nestedLoopRange": [1, 3],
    "noLoopRange": [10, 10],
    "strideRange": [1, 2],
    "expressionDepthRange": [2, 2],
    "weightCoeffForDims": 4,
    "noOfFunctions": 1,
    "allDTypes": ["int", "uint", "long", "ulong", "char"],
    "targetDTypes": ["int", "uint"],
    "useModsInOuterLoop": true,
    "repeatFactor": 20,
    "timeLimit": 3.5,
    "allowReduction": true
  }
}
