{
  "knobs": {
    "maxDims":  2,
    "noOfSingletonRange": [2, 5],
    "noOfArrayRange": [1, 1],
    "sizeRange": [100, 1000],
    "loopDepthRange": [2, 2],
    "nestedLoopRange": [1, 3],
    "noLoopRange": [1, 1],
    "expressionDepthRange": [2, 2],
    "noOfFunctions": 1,
    "allDTypes": ["int", "uint", "long", "ulong", "char"],
    "targetDTypes": ["int", "uint"],
    "useModsInOuterLoop": true,
    "repeatFactor": 20,
    "allowReduction": false
  }
}
