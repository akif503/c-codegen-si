{
  "knobs": {
    "maxDims":  1, 
    "sizeRange": [100, 100],
    "loopDepthRange": [3, 7],
    "nestedLoopRange": [1, 3],
    "noLoopRange": [1, 1],
    "expressionDepthRange": [1, 4],
    "noOfFunctions": 1,
    "allDTypes": ["int", "uint", "long", "ulong", "char"],
    "targetDTypes": ["int", "uint"],
    "useModsInOuterLoop": false,
    "repeatFactor": 50,
    "allowReduction": true
  }
}
